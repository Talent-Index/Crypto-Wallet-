{
  "name": "my-crypto-wallet",
  "version": "1.0.0",
  "description": "Example Avalanche wallet project with TypeScript snippets and tests",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p . && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand",
    "cli:create-wallet": "ts-node src/cli/createWallet.ts",
    "cli:import-mnemonic": "ts-node src/cli/importMnemonic.ts",
    "cli:send-c": "ts-node src/cli/sendC.ts",
    "cli:send-x": "ts-node src/cli/sendX.ts",
    "test:watch": "jest --watchAll",
    "start": "node dist/index.js",
    "lint": "echo \"No linter configured\""
