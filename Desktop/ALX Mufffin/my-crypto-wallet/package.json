    "ethereumjs-wallet": "^1.0.1",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "jest": "^29.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
